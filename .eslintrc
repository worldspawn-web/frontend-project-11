{
  "parser": "@babel/eslint-parser",
  "extends": ["airbnb"],
  "rules": {
    "no-unused-vars": "warn",
    "implicit-arrow-linebreak": "off",
    "no-param-reassign": "warn",
    "function-paren-newline": "off",
    "object-curly-newline": "off"
  },
  "env": {
    "browser": true,
    "node": true
  }
}
